{
  "name": "grafana-timestream-datasource",
  "version": "2.9.9",
  "description": "Load data timestream in grafana",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "yarn exec cypress install && yarn exec grafana-e2e run",
    "e2e:open": "grafana-e2e open",
    "e2e:update": "yarn exec cypress install && yarn exec grafana-e2e run --update-screenshots",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\"",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "test:coverage": "jest --coverage",
    "test:coverage:changes": "jest --coverage --changedSince=origin/main",
    "typecheck": "tsc --noEmit"
  },
  "repository": "github:grafana/timestream-datasource",
  "author": "Grafana Labs <team@grafana.com> (https://grafana.com)",
  "license": "Apache-2.0",
  "dependencies": {
    "@grafana/data": "11.3.0",
    "@grafana/experimental": "^2.1.2",
    "@grafana/runtime": "11.3.0",
    "@grafana/schema": "11.3.0",
    "@grafana/ui": "11.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tslib": "2.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@grafana/aws-sdk": "0.5.0",
    "@grafana/e2e": "11.0.7",
    "@grafana/e2e-selectors": "11.3.0",
    "@grafana/eslint-config": "^8.0.0",
    "@grafana/tsconfig": "^2.0.0",
    "@swc/core": "^1.7.40",
    "@swc/helpers": "^0.5.13",
    "@swc/jest": "^0.2.37",
    "@testing-library/jest-dom": "6.6.2",
    "@testing-library/react": "16.0.1",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.8.4",
    "@types/react-router-dom": "^5.2.0",
    "copy-webpack-plugin": "^12.0.2",
    "cspell": "^8.15.5",
    "css-loader": "^7.1.2",
    "cypress": "13.15.1",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "9.0.2",
    "glob": "^11.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^3.3.3",
    "react-select-event": "^5.3.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.80.5",
    "sass-loader": "16.0.2",
    "style-loader": "4.0.0",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.6.3",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "resolutions": {
    "@cypress/request": "3.0.1",
    "braces": "3.0.3",
    "cosmiconfig": "8.3.6",
    "debug": "4.3.5",
    "log4js": "6.9.1",
    "tough-cookie": "4.1.4",
    "underscore": "1.12.1",
    "ws": "8.17.1"
  },
  "engines": {
    "node": ">=18"
  }
}
