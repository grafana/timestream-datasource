{
  "name": "grafana-timestream-datasource",
  "version": "2.0.1",
  "description": "Load data timestream in grafana",
  "scripts": {
    "build": "grafana-toolkit plugin:build",
    "test": "jest --notify --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:changes": "jest --coverage --changedSince=origin/main",
    "dev": "grafana-toolkit plugin:dev",
    "watch": "grafana-toolkit plugin:dev --watch",
    "e2e": "grafana-e2e run",
    "e2e:open": "grafana-e2e open",
    "e2e:update": "grafana-e2e run --update-screenshots"
  },
  "repository": "github:grafana/timestream-datasource",
  "author": "Grafana Labs <team@grafana.com> (https://grafana.com)",
  "license": "Apache-2.0",
  "devDependencies": {
    "@grafana/aws-sdk": "0.0.37",
    "@grafana/data": "8.0.5",
    "@grafana/runtime": "8.0.5",
    "@grafana/toolkit": "8.0.5",
    "@grafana/ui": "8.5.2",
    "@grafana/e2e": "8.0.5",
    "@grafana/e2e-selectors": "8.0.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/lodash": "latest",
    "react-select-event": "^5.3.0",
    "tslib": "2.3.1",
    "cypress": "6.4.0"
  },
  "resolutions": {
    "rxjs": "6.6.3"
  }
}
