    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.8.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-livereload-plugin": "^3.0.2",
    "webpack-subresource-integrity": "^5.1.0",
    "webpack-virtual-modules": "^0.6.2",
    "semver": "^7.6.3"
  },
  "resolutions": {
    "braces": "3.0.3",
    "cosmiconfig": "8.3.6",
    "debug": "4.3.5",
    "log4js": "6.9.1",
    "tough-cookie": "4.1.4",
    "underscore": "1.12.1",
    "ws": "8.17.1"
  },
  "engines": {
    "node": ">=18"
  }
}
