{
  "name": "grafana-timestream-datasource",
  "version": "2.10.0",
  "description": "Load data timestream in grafana",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "playwright test",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix && prettier --write --list-different .",
    "server": "docker compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\"",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "test:coverage": "jest --coverage",
    "test:coverage:changes": "jest --coverage --changedSince=origin/main",
    "typecheck": "tsc --noEmit"
  },
  "repository": "github:grafana/timestream-datasource",
  "author": "Grafana Labs <team@grafana.com> (https://grafana.com)",
  "license": "Apache-2.0",
  "dependencies": {
    "@emotion/css": "11.13.5",
    "@grafana/data": "^11.5.2",
    "@grafana/experimental": "^2.1.6",
    "@grafana/runtime": "^11.5.2",
    "@grafana/schema": "^11.5.2",
    "@grafana/ui": "^11.5.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "^7.1.5",
    "tslib": "2.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.8",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.20.0",
    "@grafana/aws-sdk": "0.7.0",
    "@grafana/eslint-config": "^8.0.0",
    "@grafana/plugin-e2e": "^1.18.0",
    "@grafana/tsconfig": "^2.0.0",
    "@playwright/test": "^1.50.1",
    "@stylistic/eslint-plugin-ts": "^4.2.0",
    "@swc/core": "^1.11.5",
    "@swc/helpers": "^0.5.15",
    "@swc/jest": "^0.2.37",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.8",
    "@types/react-router-dom": "^5.2.0",
    "@types/testing-library__jest-dom": "6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "copy-webpack-plugin": "^13.0.0",
    "cspell": "^8.17.3",
    "css-loader": "^7.1.2",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-webpack-plugin": "^5.0.0",
    "fork-ts-checker-webpack-plugin": "9.0.2",
    "glob": "^11.0.1",
    "identity-obj-proxy": "3.0.0",
    "imports-loader": "^5.0.0",
    "jest": "^29.5.0",
