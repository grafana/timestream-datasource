{
  "name": "grafana-timestream-datasource",
  "version": "2.12.2",
  "description": "Load data timestream in grafana",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "playwright test",
    "lint": "eslint --cache .",
    "lint:fix": "yarn run lint --fix && prettier --write --list-different .",
    "server": "docker compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\"",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "test:coverage": "jest --coverage",
    "test:coverage:changes": "jest --coverage --changedSince=origin/main",
    "typecheck": "tsc --noEmit"
  },
  "repository": "github:grafana/timestream-datasource",
  "author": "Grafana Labs <team@grafana.com> (https://grafana.com)",
  "license": "Apache-2.0",
  "dependencies": {
    "@emotion/css": "11.13.5",
    "@grafana/data": "^12.1.0",
    "@grafana/experimental": "^2.1.6",
    "@grafana/runtime": "^12.1.0",
    "@grafana/schema": "^12.0.0",
    "@grafana/ui": "^12.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "^6.22.0",
    "tslib": "2.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.27.7",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@grafana/aws-sdk": "0.7.1",
    "@grafana/eslint-config": "^8.1.0",
    "@grafana/plugin-e2e": "^2.1.7",
    "@grafana/tsconfig": "^2.0.0",
    "@playwright/test": "1.53.1",
    "@stylistic/eslint-plugin-ts": "^4.4.1",
    "@swc/core": "^1.12.7",
    "@swc/helpers": "^0.5.17",
    "@swc/jest": "^0.2.39",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.15.21",
    "@types/semver": "^7.7.0",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "copy-webpack-plugin": "^13.0.0",
    "cspell": "^9.1.2",
    "css-loader": "^7.1.2",
    "dotenv": "^17.2.1",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-jsdoc": "^51.3.1",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-webpack-plugin": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "9.1.0",
    "glob": "^11.0.3",
    "identity-obj-proxy": "3.0.0",
    "imports-loader": "^5.0.0",
    "jest": "^30.0.3",
    "jest-environment-jsdom": "^30.0.2",
    "prettier": "^3.6.2",
    "react-select-event": "^5.3.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.89.2",
    "sass-loader": "16.0.5",
    "semver": "^7.6.3",
    "style-loader": "4.0.0",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "^5.3.14",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.8.3",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-livereload-plugin": "^3.0.2",
    "webpack-subresource-integrity": "^5.1.0",
    "webpack-virtual-modules": "^0.6.2"
  },
  "engines": {
    "node": ">=18"
  }
}
